### GObject introspection
-include $(INTROSPECTION_MAKEFILE)

@HAVE_INTROSPECTION_TRUE@Gegl-@GEGL_API_VERSION@.gir: libgegl-@GEGL_API_VERSION@.la Makefile

@HAVE_INTROSPECTION_TRUE@@HAVE_VALA_TRUE@gegl-@GEGL_API_VERSION@.vapi: Gegl-@GEGL_API_VERSION@.gir
@HAVE_INTROSPECTION_TRUE@@HAVE_VALA_TRUE@	$(VAPIGEN) --library=gegl-@GEGL_API_VERSION@ Gegl-@GEGL_API_VERSION@.gir

gegl-enums.c: $(srcdir)/gegl-enums.h
	glib-mkenums \
		--fhead "/* This is a generated file, do not edit directly */\n\n#include \"config.h\"\n#include <glib-object.h>\n#include \"gegl-enums.h\"" \
		--fprod "\n/* enumerations from \"@filename@\" */" \
		--vhead "GType\n@enum_name@_get_type (void)\n{\n  static GType etype = 0;\n  if (etype == 0) {\n    static const G@Type@Value values[] = {" \
		--vprod "      { @VALUENAME@, \"@valuenick@\", \"@valuenick@\" }," \
		--vtail "      { 0, NULL, NULL }\n    };\n    etype = g_@type@_register_static (\"@EnumName@\", values);\n  }\n  return etype;\n}\n\n" \
		$(srcdir)/gegl-enums.h > xgen-tec \
	&& cp xgen-tec $(@F) \
	&& rm -f xgen-tec

# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
